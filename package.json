{
  "name": "construction-material-shop",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:analyze": "ANALYZE=true next build --turbopack",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "performance:analyze": "npm run build:analyze",
    "clean": "rm -rf .next out node_modules/.cache",
    "postinstall": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "jest --testPathPattern=security",
    "security:audit": "npm audit --audit-level high",
    "security:headers": "npm run build && echo 'Check security headers manually'"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "@octokit/webhooks": "^13.9.1",
    "@prisma/client": "^6.18.0",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/html2canvas": "^0.5.35",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "dotenv": "^17.2.3",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "html2canvas": "^1.4.1",
    "jose": "^6.1.0",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "nodemailer": "^7.0.10",
    "prisma": "^6.18.0",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^4.0.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0",
    "react-hot-toast": "^2.6.0",
    "stripe": "^19.3.0",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.3",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/testing-library__jest-dom": "^5.14.9",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
